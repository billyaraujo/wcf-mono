<?xml version="1.0" encoding="UTF-8" ?>
<configuration>
    <system.serviceModel>
        <bindings>
            <netNamedPipeBinding>
                <binding name="NetNamedPipeBinding_IMyService" />
            </netNamedPipeBinding>
            <wsDualHttpBinding>
                <binding name="WSDualHttpBinding_IMyService" />
            </wsDualHttpBinding>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IMyService" />
            </basicHttpBinding>
            <netTcpBinding>
                <binding name="NetTcpBinding_IMyService" />
            </netTcpBinding>
        </bindings>
        <client>
        	<endpoint name="pipe" contract="IMyService" address="http://localhost:8000/MyService/pipe" binding="netNamedPipeBinding" bindingConfiguration="NetNamedPipeBinding_IMyService" />
        	<endpoint name="dual" contract="IMyService" address="http://localhost:8000/MyService/dual" binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_IMyService" />
        	<endpoint name="basic" contract="IMyService" address="http://localhost:8000/MyService/basic" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IMyService" />
        	<endpoint name="tcp" contract="IMyService" address="net.tcp://localhost:82/MyService/tcp" binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IMyService" />
        </client>
    </system.serviceModel>
</configuration>
